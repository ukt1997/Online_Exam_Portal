        <servlet-name>request_approval</servlet-name>
        <servlet-class>online_exam.request_approval</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reexam</servlet-name>
        <servlet-class>online_exam.reexam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>result</servlet-name>
        <servlet-class>online_exam.result</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resultmake</servlet-name>
        <servlet-class>online_exam.resultmake</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginservlet</servlet-name>
        <url-pattern>/loginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup_servlet</servlet-name>
        <url-pattern>/signup_servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_servlet</servlet-name>
        <url-pattern>/login_servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addcourses</servlet-name>
        <url-pattern>/addcourses</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addque</servlet-name>
        <url-pattern>/addque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>apply</servlet-name>
        <url-pattern>/apply</url-pattern>
    </servlet-mapping>
